var DataService = (function () {
    function DataService(http) {
        this.http = http;
    }
    DataService.prototype.getProjects = function () {
        return this.http.get("data/projects.json")
            .toPromise()
            .then(function (response) { return response.json().filter(function (p) { return p.show; }).sort(function (p1, p2) { return p1.order - p2.order; }); });
    };
    DataService.prototype.getProjectsForLanguage = function (languageId) {
        return this.http.get("data/projects.json")
            .toPromise()
            .then(function (response) { return response.json().filter(function (p) { return p.show && p.languages.includes(languageId); }).sort(function (p1, p2) { return p1.order - p2.order; }); });
    };
    DataService.prototype.getProject = function (id) {
        return this.getProjects().then(function (projects) { return projects.find(function (p) { return p.show && p.id === id; }); });
    };
    DataService.prototype.getLanguages = function () {
        return this.http.get("data/languages.json")
            .toPromise()
            .then(function (response) { return response.json().filter(function (l) { return l.show; }).sort(function (p1, p2) { return p1.order - p2.order; }); });
    };
    DataService.prototype.getLanguagesForProject = function (project) {
        return this.http.get("data/languages.json")
            .toPromise()
            .then(function (response) { return response.json().filter(function (l) { return l.show && project.languages.includes(l.id); }).sort(function (p1, p2) { return p1.order - p2.order; }); });
    };
    DataService.prototype.getLanguage = function (id) {
        return this.getLanguages().then(function (languages) { return languages.find(function (l) { return l.show && l.id === id; }); });
    };
    DataService.prototype.getPrograms = function () {
        return this.http.get("data/programs.json")
            .toPromise()
            .then(function (response) { return response.json(); });
    };
    return DataService;
}());
